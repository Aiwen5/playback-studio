<header>
    <h1 class="large-wordmark">Playback Studio</h1>
</header>

<div class="cta">
    <a class="btn" href="/add-album">Add to Collection</a>
</div>

<section class="album-list">
    <% albums.forEach(album => { %>
        <div class="album-card">
            <a href="/album/<%= album.id %>">
                <img src="<%= album.image_url %>" alt="<%= album.title %>">
                <p><%= album.artist %> - <%= album.title %></p>
            </a>
        </div>
    <% }) %>
</section>